@model MvcApplication1.Models.QueueModel

@{

    ViewBag.Title = "Queue Overview";
    List<MvcApplication1.get_Queue_Status_Result> result = MvcApplication1.Controllers.QueueController.GetQueueNames();
}

<h1>Queue Management</h1>


<div class="row" >
    <div class="col-mid-3" style="overflow:hidden; position:relative">
        <table class="table table-hover">
            <thead>
                <tr><th>Queue</th><th>Size</th></tr>
            </thead>
            <tbody>
                @for (int i = 0; i < result.Count; i++)
                {
                    <tr onclick="goToQueueDetail('@result[i].Name')"><td>@result[i].Name</td><td>@result[i].size</td></tr>
                }

            </tbody>
        </table>
    </div>
</div>

