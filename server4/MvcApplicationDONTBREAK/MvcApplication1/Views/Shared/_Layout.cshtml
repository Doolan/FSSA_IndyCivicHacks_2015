<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="~/Images/fssa.jpeg" rel="shortcut icon" type="image/jpeg" />
    <meta name="viewport" content="width=device-width" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/Scripts/jquery-1.9.1.js")
    @Scripts.Render("~/Scripts/bootstrap.min.js")
    @Styles.Render("~/Content/bootstrap.min.css")
</head>
    <body>
        <header>
            <nav class="navbar navbar-inverse navbar-static-top">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbarcollapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="#">
                            <img alt="Brand" src="~/Images/fssaSMALL.png" />
                        </a>
                    </div>
                    <div class="collapse navbar-collapse" id="navbarcollapse">
                        <ul class="nav navbar-nav">
                            <li class="active"><a href="#">Home </a></li>
                            <li>
                                @Html.ActionLink("Queue", "QueueDetail", "Queue");
                            </li>
                            @*<li>
                                @Html.ActionLink("About", "About");
                            </li> #4d7c36
                            <li><a href="#">Officers</a></li>
                            <li>
                                @Html.ActionLink("Mission & Creed", "MissionCreed")
                                <!--<a href="#">Mission &amp; Creed</a>-->
                            </li>*@
                        </ul>
                        <section id="login">
                            @Html.Partial("_LoginPartial")
                        </section>
                    </div>


                </div>
            </nav>
        </header>
        <div id="body">
            <section class="content-wrapper main-content clear-fix">
                @RenderBody()
            </section>
        </div>
        <footer>
            <div class="content-wrapper">
                <div class="float-left">
                    <p>&copy; @DateTime.Now.Year -Queue Manager</p>
                </div>
            </div>
        </footer>

        @Scripts.Render("~/bundles/jquery")
        @RenderSection("scripts", required: false)
    </body>
</html>
